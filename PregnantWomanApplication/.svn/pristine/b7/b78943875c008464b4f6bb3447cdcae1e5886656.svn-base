<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    android:clickable="true">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fadingEdge="none"
        android:fillViewport="true"
        android:layout_above="@+id/buttomLayout">

        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <!--Title-->

            <RelativeLayout
                android:id="@+id/rltitlevaccin"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical" >

                <com.widevision.pregnantwoman.model.CustomTextViewBold
                    android:id="@+id/txtvaccin"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:textColor="@color/black"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Vaccination Chart"
                    android:gravity="center" />

            </RelativeLayout>

            <View
                android:id="@+id/views"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/accent_material_light" />

            <!---->

            <!--Name-->

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:padding="5dp">

                <com.rengwuxian.materialedittext.MaterialEditText
                    android:id="@+id/etnamevaccin"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Name Of Baby"
                    android:textColor="@color/text_color"
                    android:editable="false"
                    android:focusable="false"
                    android:inputType="textPersonName"
                    android:singleLine="true"
                    app:met_floatingLabel="highlight"
                    android:clickable="false"
                    android:layout_marginTop="5dp" />

                <View
                    android:id="@+id/view0"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/accent_material_light" />


            </LinearLayout>

            <!---->
            <!--Vaccin Table-->

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_margin="10dp"
                android:layout_marginBottom="20dp"
                android:padding="5dp">

                <TableLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edittextborder">

                    <!--Table Heading-->
                    <TableRow
                        android:id="@+id/tabletitlevaccin"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:layout_margin="3dp"
                        android:padding="2dp"
                        android:weightSum="5">

                        <com.widevision.pregnantwoman.model.CustomTextViewBold
                            android:id="@+id/txtvaccinname"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:textColor="@color/black"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:text="Vaccination Name"
                            android:gravity="left|center"/>

                        <com.widevision.pregnantwoman.model.CustomTextViewBold
                            android:id="@+id/txtduedate"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textColor="@color/black"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:text="Due Date"
                            android:gravity="left|center"/>

                        <com.widevision.pregnantwoman.model.CustomTextViewBold
                            android:id="@+id/txtdoneornot"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:textColor="@color/black"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:text="@string/donenotdone"
                            android:gravity="left|center" />

                    </TableRow>
                    <View
                        android:id="@+id/view1"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/accent_material_light" />

